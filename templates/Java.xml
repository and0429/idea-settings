<templateSet group="Java">
  <template name="fori" value="for(int $INDEX$ = 0; $INDEX$ &lt; $LIMIT$; $INDEX$++) {&#10;    $END$&#10;}" description="Create iteration loop" toReformat="true" toShortenFQNames="true">
    <variable name="INDEX" expression="suggestIndexName()" defaultValue="" alwaysStopAt="true" />
    <variable name="LIMIT" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="/T" value="/**&#10; * $END$&#10; * &#10; * @author     zhangwenyu&#10; * @date       $date$&#10; * @className  $className$&#10; * @version    1.0&#10; * &#10; */" description="java类描述" toReformat="true" toShortenFQNames="true">
    <variable name="date" expression="date()-time()" defaultValue="" alwaysStopAt="true" />
    <variable name="className" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>